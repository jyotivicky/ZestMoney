    <build>   
    <plugins>
      <plugin>
       <groupId>org.apache.maven.plugins</groupId>
         <artifactId>maven-surefire-plugin</artifactId>
          <version>2.19.1</version>
          
           <configuration>
           
           <includes>
           <excludes>**/*Runner.java</excludes>>
           </includes>
         
            <source>1.8</source>
             <target>1.8</target>
            </configuration>
           </plugin>
           
           <!--*********************** For JVM Report ****************************-->
			<plugin>

				<groupId>net.masterthought</groupId>
				<artifactId>maven-cucumber-reporting</artifactId>
				<version>2.8.0</version>
				    <executions>
					<execution>
						<id>execution</id>
						<phase>verify</phase>
						<goals>
						<goal>generate</goal>
						</goals>
						<configuration>
						
						    <!-- JVM REPORT HEADER -->
							<projectName>API Cucumber Framework</projectName>
							<!-- ******** -->
							
							<!-- JVM REPORT PATH IN TARGET FOLDER -->
							<outputDirectory>${project.build.directory}/JVM_report</outputDirectory>
							<!-- ******** -->
							
							<!-- CUCUMBER JSON FILE PATH IN TARGET FOLDER -->
							<cucumberOutput>${project.build.directory}/cucumber.json</cucumberOutput>
							<!-- ******** -->
							
							<skippedFails>true</skippedFails>
							<enableFlashCharts>false</enableFlashCharts>
						</configuration>
					    </execution>
				        </executions>				
			</plugin>		
           <!-- ********************************************************************* -->
        
          </plugins>
          </build>
          
